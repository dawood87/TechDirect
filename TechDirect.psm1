		)
		
		if ($Warranty)
		{
			if (Test-Path "$env:LOCALAPPDATA\TechDirect\Token-W.json")
			{
				try { $TokenInfo = Get-Content $env:LOCALAPPDATA\TechDirect\Token-W.json | ConvertFrom-Json }
				catch
