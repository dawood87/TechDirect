			$ErrorRecord = New-Object System.Management.Automation.ErrorRecord($($ResponseMessage.faultstring), "$statuscode", $ErrCategory, $Null)
			$PSCmdlet.ThrowTerminatingError($ErrorRecord)
		}
	}
	
