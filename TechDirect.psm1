		</api:ResubmitDispatch>
	</soapenv:Body>
</soapenv:Envelope>
"@
		
		
		$DellToken = Get-TDAccessToken -Dispatch
		if ($DellToken.sandbox)
		{ $endpoint = "https://apigtwb2cnp.us.dell.com/Sandbox/support/dispatch/v3/service" }
